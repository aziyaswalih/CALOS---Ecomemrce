<script>
    async function qtyCheck(input,index) {
    const row = input.closest('tr'); // Get the closest table row to the input
    const productPriceElement = row.querySelector('.price span');
    const totalPriceElement = row.querySelector('.total-price');
    totalPriceElement.style.display = 'block'
    const quantity = parseInt(input.value, 10) || 0; // Default to 0 if not a valid number
    const productId = input.dataset.productId; // Assuming data-productId attribute holds product ID
    const size = document.getElementById(`size${index}`).textContent
    console.log(size);
    const maxQuantity = await getAvailableStock(productId);
    async function getAvailableStock(productId) {
    const response = await fetch(`/product/availableStock?id=${productId}&size=${size}`); // Replace with your API endpoint
    const data = await response.json();
    return data.stock; // Assuming the stock data is in 'stock' property
    }

    input.max = maxQuantity<10?maxQuantity:10
    // =============ivide vere
    const max = parseInt(input.max, 10);
    const min = parseInt(input.min, 10);
    
    if (quantity > max) {
      input.value = max; 
      displayErrorMessage(input);
    }
    else if (quantity < min) {
      input.value = min;
      displayErrorMessage(input);
    } else {
      // Update the subtotal for this item
      const productPrice = parseFloat(productPriceElement.textContent.replace('₹', ''));
      const totalPrice = (quantity * productPrice).toFixed(2);
      totalPriceElement.textContent = `₹${totalPrice}`;
  
  
      // ============= update quantity===========================================================================
    let id = document.getElementById('cartId').value;
    let totalProducts= document.getElementById('totalProducts').value
    let cartTotalAmountInput = document.getElementById('cartTotalAmountInput').value;
    const form=document.getElementById('form')
    
  
    // Get all quantities from input fields
    let quantities = [];
    for (let i = 0; i < totalProducts; i++) {
      let qty = parseInt(document.getElementById(`qtyInput${i}`).value, 10) || 0; // Default to 0 if invalid
      quantities.push(qty);
    }
  
    // Check if any quantities are invalid (0 or negative)
    if (quantities.some(qty => qty <= 0)) {
      alert('Please enter valid quantities (greater than zero) for all products.');
      return false; // Prevent form submission
    }
  
  
    const data = {
      id: id,
      totalPrice: cartTotalAmountInput,
      quantities: quantities
    };
  
    try {
      const response = await fetch("/product/cart/quantity", {
        method: "PUT",
        body: JSON.stringify(data),
        headers: {
                     'Content-Type': 'application/json'
                  }
      });
  
      if (response.ok) {
          console.log("ok");
        
        
          console.log("update quantity");
          window.location.reload()
      
      
      } else {
        console.error("error loading order page", response.statusText);
      }
    } catch (error) {
      console.error("There was a problem with the fetch operation:", error);
    }
  
      
      // ==============update quantity end here ============
      // Update the cart total
      updateCartTotalPrice();
    
  }
}

    function displayErrorMessage(input) {
    const errorMessage = input.nextElementSibling;
    errorMessage.style.display = 'block';
    setTimeout(() => {
        errorMessage.style.display = 'none';
    }, 2000); 
  }


  function updateItemTotalPrice(productId, quantity) {
  const itemTotalPriceElement = document.querySelector(`.total-price[data-product-id="${productId}"]`);
  const productPrice = parseFloat(itemTotalPriceElement.textContent.replace('₹', '')); // Extract price, remove ₹
  const totalPrice = (quantity * productPrice).toFixed(2);

  itemTotalPriceElement.textContent = `₹${totalPrice}`; 
}

function updateCartTotalPrice() {
  const cartTotalAmountInput = document.getElementById('cartTotalAmountInput');
  const cartTotalAmount = document.getElementById('cartTotalAmount');

  let total = 0;
  document.querySelectorAll('.total-price').forEach(item => {
    const priceText = item.textContent.replace('₹', ''); // Remove ₹ symbol before parsing
    total += parseFloat(priceText) || 0; // Handle cases where priceText might not be a valid number
  });
  cartTotalAmountInput.value = `₹${total.toFixed(2)}`; 
  cartTotalAmount.textContent = total.toFixed(2); // Update the other span element as well
}

window.addEventListener('load', updateCartTotalPrice);


function validateForm() {
    // Get the value of the cart total input field
    var cartTotalAmountInput = document.getElementById('cartTotalAmountInput').value;
    
    // Convert the value to a number
    var totalAmount = parseFloat(cartTotalAmountInput);

    // Check if the value is not a valid number or less than or equal to 0
    if (isNaN(totalAmount) || totalAmount <= 0) {
        Swal.fire({
            icon: 'error',
            title: 'Oops...',
            text: 'Add Minimum One Product!'
        });
        return false; // Prevent form submission
    }
    return true; // Allow form submission
}


