<script>
    async function validateCoupon(event) {
      event.preventDefault(); // Prevent form from submitting normally
    
      const form = document.getElementById('couponForm');
      const couponCode = form.couponCode.value;
    
      if (!couponCode) {
        alert('Please select a coupon.');
        return;
      }
    
      try {
        const response = await fetch('/apply-coupon', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json',
          },
          body: JSON.stringify({ couponCode }),
        });
    
        if (!response.ok) {
          throw new Error('Network response was not ok');
        }
    
        const result = await response.json();
    
        if (result.success) {
          document.getElementById('total_amount').innerText = result.newTotal;
          Swal.fire({
                        title: 'Applied!',
                        text: `Coupon ${couponCode} applied successfully!`,
                        icon: 'success',
                        confirmButtonText: 'OK'
                    })
        //   alert(`Coupon ${couponCode} applied successfully!`);
        } else {
          alert(result.message);
        }
      } catch (error) {
        alert('There was a problem with the coupon validation.');
        console.error('Error:', error);
      }
    }
    </script>
    

