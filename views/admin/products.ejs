                  <ul class="pagination justify-content-start">
                    <% if (page > 1) { %>
                      <li class="page-item">
                        <a class="page-link" href="/admin/products?page=<%= page - 1 %>">
                          <i class="material-icons md-chevron_left"></i>
                        </a>
                      </li>
                    <% } %>
              
                    <% for (let i = 1; i <= totalPages; i++) { %>
                      <%
                        // Calculate the starting and ending page numbers for the visible page range
                        const startIndex = Math.max(Math.min(page - 2, totalPages - 4), 1);
                        const endIndex = Math.min(Math.max(page + 2, 4), totalPages);
                      %>
              
                      <% if (i >= startIndex && i <= endIndex) { %>
                        <li class="page-item <%= page == i ? 'active' : '' %>">
                          <a class="page-link" href="/admin/products?page=<%= i %>"><%= i %></a>
                        </li>
                      <% } else if (i === 1 && page > 3) { %>  <li class="page-item">
                          <a class="page-link" href="/admin/products?page=1">
                            <i class="material-icons md-fast_rewind"></i>
                          </a>
                        </li>
                      <% } else if (i === totalPages && page < totalPages - 2) { %>  <li class="page-item">
                          <a class="page-link" href="/admin/products?page=<%= totalPages %>">
                            <i class="material-icons md-fast_forward"></i>
                          </a>
                        </li>
                      <% } %>
                    <% } %>
              
                    <% if (page < totalPages) { %>
                      <li class="page-item">
                        <a class="page-link" href="/admin/products?page=<%= page + 1 %>">
                          <i class="material-icons md-chevron_right"></i>
                        </a>
                      </li>
                    <% } %>
                  </ul>
